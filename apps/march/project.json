{
	"projectType": "application",
	"root": "apps/march",
	"sourceRoot": "apps/march/src",
	"prefix": "march",
	"targets": {
		"build": {
			"executor": "@angular-devkit/build-angular:browser",
			"outputs": ["{options.outputPath}"],
			"options": {
				"outputPath": "dist/apps/march",
				"index": "apps/march/src/index.html",
				"main": "apps/march/src/main.ts",
				"polyfills": "apps/march/src/polyfills.ts",
				"tsConfig": "apps/march/tsconfig.app.json",
				"inlineStyleLanguage": "scss",
				"assets": [
					"apps/march/src/favicon.ico",
					"apps/march/src/assets",
					{
						"glob": "**/*",
						"input": "./libs/authentication/src/assets",
						"output": "./assets"
					}
				],
				"styles": [
					"./node_modules/bootstrap/scss/bootstrap.scss",
					"./node_modules/@angular/material/prebuilt-themes/pink-bluegrey.css",
					"apps/march/src/styles.scss"
				],
				"scripts": []
			},
			"configurations": {
				"production": {
					"budgets": [
						{
							"type": "initial",
							"maximumWarning": "500kb",
							"maximumError": "2mb"
						},
						{
							"type": "anyComponentStyle",
							"maximumWarning": "2kb",
							"maximumError": "10kb"
						}
					],
					"fileReplacements": [
						{
							"replace": "apps/march/src/environments/environment.ts",
							"with": "apps/march/src/environments/environment.prod.ts"
						}
					],
					"aot": true,
					"buildOptimizer": true,
					"optimization": {
						"scripts": true,
						"styles": {
							"minify": true,
							"inlineCritical": true
						},
						"fonts": true
					},
					"outputHashing": "all"
				},
				"development": {
					"buildOptimizer": false,
					"optimization": false,
					"vendorChunk": true,
					"extractLicenses": false,
					"sourceMap": true,
					"namedChunks": true
				}
			},
			"defaultConfiguration": "production"
		},
		"serve": {
			"executor": "@angular-devkit/build-angular:dev-server",
			"configurations": {
				"production": {
					"browserTarget": "march:build:production"
				},
				"development": {
					"browserTarget": "march:build:development"
				}
			},
			"defaultConfiguration": "development",
			"options": {
				"proxyConfig": "apps/march/proxy.conf.json"
			}
		},
		"extract-i18n": {
			"executor": "@angular-devkit/build-angular:extract-i18n",
			"options": {
				"browserTarget": "march:build"
			}
		},
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"options": {
				"lintFilePatterns": [
					"apps/march/src/**/*.ts",
					"apps/march/src/**/*.html"
				]
			}
		},
		"test": {
			"executor": "@nrwl/jest:jest",
			"outputs": ["coverage/apps/march"],
			"options": {
				"jestConfig": "apps/march/jest.config.js",
				"passWithNoTests": true
			}
		}
	},
	"tags": []
}
